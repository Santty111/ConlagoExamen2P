<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ConlagoExamen2P.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ConlagoExamen2P"
    Shell.FlyoutBehavior="Disabled"
    Title="ConlagoExamen2P">

    <TabBar>
        <ShellContent
            Title="Velcoidades Santiago Conlago"
            ContentTemplate="{DataTemplate local:MainPage}"
            Icon="{OnPlatform 'velocidad.png', iOS='velocidad.png', MacCatalyst='velocidad.png'}" />

        <ShellContent
            Title="Acerca de"
            ContentTemplate="{DataTemplate local:Velocidad}"
            Icon="{OnPlatform 'icon_about.png', iOS='icon_about_ios.png', MacCatalyst='icon_about_ios.png'}" />
    </TabBar>

</Shell>
